extern fn puts(message: *c_char) -> i32;

pub fn main() -> i32 {
    let i: i32 = 10;
    let j: i32 = 10;
    let k: i32 \= add(i, j);

    return k;
}

fn add(a: i32, b: i32) -> i32 {
    return a + b;
}

fn sub(a: i32, b: i32) -> i32 {
    return a - b;
}

fn mul(a: i32, b: i32) -> i32 {
    return a * b;
}

fn div(a: i32, b: i32) -> i32 {
    return a / b;
}
