extern fn puts(message: *u8) -> i32;

pub fn main() -> i32 {
    if (add(1, 2) - 1) {
	puts("WEEEE!");
    }

    return 0;
}

pub fn add(a: i32, b: i32) -> i32 {
    return a + b;
}

pub fn sub(a: i32, b: i32) -> i32 {
    return a - b;
}

fn mul(a: i32, b: i32) -> i32 {
    return a * b;
}

fn div(a: i32, b: i32) -> i32 {
    return a / b;
}
